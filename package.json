    "url": "https://github.com/elmirkouliev/react-googlemaps-crossstreet/issues"
  },
  "homepage": "https://github.com/elmirkouliev/react-googlemaps-crossstreet#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "enzyme-adapter-react-16": "^1.0.1",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
